    <environments>Default</environments>
    <formulas>
        <name>CalculateStatusDurationForNewEnquiryInDays</name>
        <dataType>Number</dataType>
        <expression>({!$Flow.CurrentDateTime}-{!$Record.CreatedDate})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalculateStatusDurationForNewEnquiryInHours</name>
        <dataType>Number</dataType>
        <expression>({!$Flow.CurrentDateTime}-{!$Record.CreatedDate})*24</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalculatingAge</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record__Prior.Stage__c}) + &quot; : &quot; &amp; TEXT({!CalculatingStatusDurationInHours}) &amp; &quot; Hours&quot;</expression>
    </formulas>
    <formulas>
        <name>CalculatingAgeForNewEnquiry</name>
        <dataType>String</dataType>
        <expression>TEXT({!$Record__Prior.Stage__c}) + &quot; : &quot; &amp; TEXT({!CalculateStatusDurationForNewEnquiryInHours}) &amp; &quot; Hours&quot;</expression>
    </formulas>
    <formulas>
        <name>CalculatingStatusDurationInDays</name>
