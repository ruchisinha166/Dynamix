        <expression>({!$Flow.CurrentDateTime}-{!$Record__Prior.Last_Status_Change__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalculatingStatusDurationInHours</name>
        <dataType>Number</dataType>
        <expression>({!$Flow.CurrentDateTime}-{!$Record__Prior.Last_Status_Change__c})*24</expression>
        <scale>2</scale>
    </formulas>
