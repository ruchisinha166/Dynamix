<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Landline_no_validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(IsPersonAccount,NOT(ISBLANK(Landline__c)),NOT(ISNUMBER(Landline__c)))</errorConditionFormula>
    <errorDisplayField>Landline__c</errorDisplayField>
    <errorMessage>Landline number can contains only number.</errorMessage>
</ValidationRule>
