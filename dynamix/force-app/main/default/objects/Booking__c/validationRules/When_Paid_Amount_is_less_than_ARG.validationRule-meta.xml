<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>When_Paid_Amount_is_less_than_ARG</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Registration_Status__c ,&apos;Completed&apos;),(Payment_Received_Agreement__c &lt;  Agreement_Value__c * 0.1))</errorConditionFormula>
    <errorMessage>Your paid amount is 10% less than Agreement Amount</errorMessage>
</ValidationRule>
