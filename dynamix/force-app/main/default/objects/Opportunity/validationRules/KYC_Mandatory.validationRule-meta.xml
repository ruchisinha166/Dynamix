<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>KYC_Mandatory</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
    PRIORVALUE(Is_Booking_Form_Submitted_by_Customer__c)   = true,
    NOT( KYC_Completed__c )
)</errorConditionFormula>
    <errorDisplayField>KYC_Completed__c</errorDisplayField>
    <errorMessage>KYC  Completed is Mandatory</errorMessage>
</ValidationRule>
