<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StampBy_Mandatory</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
      PRIORVALUE(Is_Booking_Form_Submitted_by_Customer__c)  = true,
    ISBLANK(TEXT( Stamp_Duty_Payable_By__c ))
)</errorConditionFormula>
    <errorDisplayField>Stamp_Duty_Payable_By__c</errorDisplayField>
    <errorMessage>Stamp Duty Payable By is Mandatory</errorMessage>
</ValidationRule>
