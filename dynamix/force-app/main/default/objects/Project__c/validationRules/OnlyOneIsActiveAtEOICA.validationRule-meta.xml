<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OnlyOneIsActiveAtEOICA</fullName>
    <active>true</active>
    <errorConditionFormula>AND( EOI__c = true, CA__c =true)</errorConditionFormula>
