                                    <apex:input type="text" value="{!primaryAccountLandline}" html-oninput="checkPrimaryAccountLandlineStr();" id="primaryAccountLandline"/>
                                    <apex:actionSupport event="onchange" reRender="primaryAccountLandlineError" status="status"/>
                                </div>
                                <div style="display:inline-block;color:red;" id="primaryAccountLandlineError"></div>
