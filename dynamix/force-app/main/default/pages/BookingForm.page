<apex:page standardController="Booking__c"  extensions="BookingFormVfpController" docType="html-5.0" showHeader="false" applyBodyTag="false" applyHtmlTag="false"  standardStylesheets="false">
    <html>  
        <head>   
            <style type="text/css">
                body {
                font-family: Helvetica, sans-serif;
                font-size: 11px; 
                }
                .fontFamily{
                /*font-family:Calibri;
                font-size:14px;*/
                font-family: Helvetica, sans-serif;
                font-size: 11px; 
                }
                .required:after {
                content:" *";
                color: red;
                }
            </style>    
            <script type="text/javascript">
            window.reload();
            </script>
        </head> 
        <body onload="onloadf()" class="fontFamily">            
            <apex:form id="frm">
                
                <apex:pageBlock >
                    <div style="background-color:#5F6A6A;padding-top:5px;font-size:18px;padding-bottom:5px;color:white;text-align:center">
                        <b>Booking Application Form </b> 
                    </div>
                    <div style="padding-top:12px;padding-bottom:5px;text-align:left;">
                        <div style="width:25%;display:inline-block;" class="required">
                            Booking Application Form For:
                        </div>
                        <div style="display:inline-block;" > 
                            <!-- <apex:inputField value="{!opportunityObj.Nature_of_Purchase__c}"/>-->
                            <apex:selectList size="1" value="{!selectedPurchasePurpose}" disabled="true"> 
                                <apex:selectOptions value="{!selPurchasePurposeOption}"/> 
                                <apex:actionSupport event="onchange" action="{!onChangePurchasePurpose}" />
                            </apex:selectList>
                        </div>
                    </div>
                    <div style="padding-bottom:12px;text-align:left;">
                        
                        <apex:actionRegion > 
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">  
                                
                                <div style="width:25%;display:inline-block;" class="required">
                                    EOI/CA Booking
                                </div>
                                <div style="display:inline-block;">                                     
                                    <apex:selectList size="1" value="{!selectedEOIBooking}" disabled="{!isDisableEOIBooking}"> 
                                        <apex:selectOptions value="{!eoiBookingOptions}"/>  
                                        <apex:actionSupport event="onchange" reRender="eoiSection" action="{!onChangeEoiBooking}" />
                                    </apex:selectList>
                                </div>
                                
                            </div>                         
                            <apex:outputPanel id="eoiSection">                               
                                <apex:pageBlock rendered="{!showEoiBookingDateandAmt}">
                                    <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                        <div style="width:25%;display:inline-block;"> EOI/CA Date</div>
                                        <div style="width:25%;display:inline-block;" > 
                                            <apex:input type="date" disabled="{!isDisableEOIBooking}" value="{!eoiDate}"/>
                                        </div>
                                        <!--div style="display:inline-block;"> EOI Amount</div>
                                        <div style="display:inline-block;"> 
                                            <apex:input type="number" value="{!eoiAmount}"/>
                                        </div-->
                                    </div>   
                                </apex:pageBlock>                              
                            </apex:outputPanel>                            
                        </apex:actionRegion>
                    </div>
                    <!--  Section 1 -->
                    <apex:pageBlock rendered="{!showSection1}">
                        <div style="background-color:#D5D8DC;padding-top:5px;padding-bottom:5px;color:black;text-align:center">
                            <b>Section 1: Details of First Applicant </b> 
                        </div>
                        <div style="margin-top:10px;margin-bottom:10px;">
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;" class="{!IF(submittedBy == 'customer', 'required', '')}"> Full Name of the Applicant</div>
                                <div style="width:7%;display:inline-block;"> 
                                    <!-- <apex:input type="text" value="{!primaryAccountSalutation}" title="Title" html-placeholder="Title"/>-->
                                    <apex:selectList size="1" value="{!primaryAccountSalutation}"> 
                                        <apex:selectOptions value="{!primaryAccountSalutationOption}"/>                          
                                    </apex:selectList>                         
                                </div>
                                <div style="width:15%;display:inline-block;"> 
                                    <apex:input type="text" value="{!primaryAccountLastName}" title="Last Name" html-placeholder="Last Name"/>
                                </div>
                                <div style="width:15%;display:inline-block;">
                                    <apex:input type="text" value="{!primaryAccountMiddleName}" title="Middle Name" html-placeholder="Middle Name"/>
                                </div>
                                <div style="width:15%;display:inline-block;"> 
                                    <apex:input type="text" value="{!primaryAccountFirstName}" title="First Name" html-placeholder="First Name" />
                                </div>
                            </div>
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;" class="{!IF(submittedBy == 'customer' && showAadhaarSection == true, 'required', '')}">
                                    Date of Birth
