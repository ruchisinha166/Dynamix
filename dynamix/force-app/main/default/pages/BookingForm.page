                                        <div style="display:inline-block;"> 
                                            
                                            <apex:selectList size="1" value="{!selectedPrimaryAccPermAddCountry}"> 
                                                <apex:selectOptions value="{!selectedPrimaryAccPermAddCountryOptions}"/>
