                                        <div style="display:inline-block;"> 
                                            <apex:selectList size="1" value="{!selectedPrimaryAccPermAddState}"> 
                                                <apex:selectOptions value="{!selectedPrimaryAccPermAddStateOptions}"/>  
                                            </apex:selectList>
                                        </div>
                                    </apex:outputPanel>
                                </div>
                                <div style="width:30%;display:inline-block;">  
                                    <apex:actionRegion > 
                                        <div style="width:20%;display:inline-block;" class="{!IF(submittedBy == 'customer', 'required', '')}">
                                            Country
