                                    </div>
                                    <div style="padding-left:50px;display:inline-block;"></div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccPassportFile}" filename="{!primaryAccPassportFileName}"/>
                                    </div>
                                </div>
                                <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                    <div style="width:25%;display:inline-block;" >
                                        OCI/PIO Card
                                    </div>
                                    <div style="width:10%;display:inline-block;"> 
                                        <apex:input type="text" value="{!primaryAccountOCIPIOCard}"/>
                                    </div>
                                    <div style="padding-left:50px;display:inline-block;"></div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccOCIPIOFile}" filename="{!primaryAccOCIPIOFileName}"/>
                                    </div>
                                </div>
                                <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                    <div style="width:25%;display:inline-block;" >
                                        PAN Details
                                    </div>
                                    <div style="width:10%;display:inline-block;"> 
                                        
                                        <apex:input type="text" value="{!primaryAccountPanDetails}" html-oninput="checkPrimaryAccountPanDetails();" id="primaryAccountPanDetails"/>
                                        <apex:actionSupport event="onchange" reRender="primaryAccountPanDetailsError" status="status"/>
                                    </div>   
                                    <div style="padding-left:50px;display:inline-block;color:red;" id="primaryAccountPanDetailsError"></div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccPanDetailsFile}" filename="{!primaryAccPanDetailsFileName}"/>
                                    </div>
                                </div>    
                                <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                    <div style="width:25%;display:inline-block;" class="{!IF(submittedBy == 'customer', 'required', '')}">
                                        Address Proof
                                    </div>
                                    <div style="width:10%;display:inline-block;"> 
                                        <apex:input type="text" value="{!primaryAccountAddressProof}"/>
                                    </div>
                                    <div style="padding-left:50px;display:inline-block;"></div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccAddProofFile}" filename="{!primaryAccAddProofFileName}"/>
                                    </div>
                                </div>
                            </apex:pageBlock>
                            
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;" class="{!IF(submittedBy == 'customer', 'required', '')}"> Mobile Number
                                </div>
                                <div style="display:inline-block;"> 
                                    <apex:input type="text" value="{!primaryAccountMobileNo}" html-oninput="checkPrimaryAccountMobileNoStr();" id="primaryAccountMobileNo"/>
                                    <apex:actionSupport event="onchange" reRender="err1" status="status"/>
                                </div>
                                <div style="display:inline-block;color:red;" id="err1"></div>
                            </div>
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;" class="{!IF(submittedBy == 'customer', 'required', '')}">Email</div>
                                
                                <div style="display:inline-block;"> 
                                    <apex:input type="email" value="{!primaryAccountEmail}" html-oninput="checkPrimaryAccountEmailStr();" id="primaryAccountEmail"/>
                                    <apex:actionSupport event="onchange" reRender="err3" status="status"/>
                                </div>
                                <div style="display:inline-block;color:red;" id="err3"></div>
                                
                            </div>
                            <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;">Landline</div>
