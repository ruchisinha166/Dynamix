                                        <apex:actionSupport event="onchange" reRender="primaryAccountAadhaarCardError" status="status"/>
                                    </div>   
                                    <div style="padding-left:50px;display:inline-block;">
                                        
                                    </div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccAadharCardFile}" filename="{!primaryAccAadharCardFileName}"/>
                                    </div>
                                    
                                </div>
                                    </apex:actionRegion>
                                </apex:outputPanel>
                                <apex:outputPanel rendered="{!showGStSection}">
                                    <apex:actionRegion >
                                 <div style="text-align:left;padding-top:2px;padding-bottom:2px;">
                                <div style="width:25%;display:inline-block;" >
                                    GST Number
                                </div>
                                <div style="width:10%;display:inline-block;"> 
                                    
                                    <apex:input type="text" value="{!thirdAccountGstNumber}"  id="thirdAccountGstNumber"/>
                                </div>   
                                <div style="padding-left:50px;display:inline-block;color:red;" id="primaryAccountAadhaarCardError"></div>

                                <div style="margin-left:94px;display:inline-block;"> 
