                                        <apex:actionSupport event="onchange" reRender="primaryAccountPanNumberError" status="status"/>
                                    </div>   
                                    <div style="padding-left:50px;display:inline-block;color:red;" id="primaryAccountPanNumberError"></div>
                                    <div style="margin-left:94px;display:inline-block;"> 
                                        <apex:inputFile value="{!primaryAccPanCardFile}" filename="{!primaryAccPanCardFileName}"  />
                                    </div>
                                    
                                </div>
                                <apex:outputPanel rendered="{!showAadhaarSection}">
